{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"es6"
		],
		"allowJs": true,
		"outDir": "build",
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"declaration": true,
		"composite": true,
		"declarationMap": true,
		"moduleResolution": "node"
	},
	"references": []
}
